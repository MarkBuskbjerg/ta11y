{
	"name": "ta11y",
	"version": "0.1.0",
	"description": "A simple 11ty starter project with complete TailwindCSS integration",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
		"build:11ty": "eleventy --quiet",
		"build:rollup": "rollup -c",
		"clean": "rimraf public",
		"dev": "npm-run-all clean build:* -p dev:*",
		"dev:11ty": "eleventy --serve --quiet",
		"dev:rollup": "rollup -c -w"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MarkBuskbjerg/ta11y.git"
	},
	"keywords": [
		"11ty",
		"tailwindcss",
		"ssg",
		"jamstack"
	],
	"author": "Mark Buskbjerg",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/MarkBuskbjerg/ta11y/issues"
	},
	"homepage": "https://github.com/MarkBuskbjerg/ta11y#readme",
	"devDependencies": {
		"@11ty/eleventy": "^0.10.0",
		"@fullhuman/postcss-purgecss": "^2.2.0",
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-replace": "^2.3.2",
		"autoprefixer": "^9.7.6",
		"base-64": "^0.1.0",
		"cross-env": "^7.0.2",
		"npm-run-all": "^4.1.5",
		"postcss-import": "^12.0.1",
		"postcss-nested": "^4.2.1",
		"request": "^2.88.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.8.0",
		"rollup-plugin-postcss": "^3.1.1",
		"rollup-plugin-terser": "^5.3.0",
		"tailwindcss": "^1.4.4"
	}
}
